{
  "name": "todo",
  "version": "0.1.0",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "bsc-flags": ["-bs-super-errors"],
  "package-specs": [
    {
      "module": "es6"
    }
  ],
  "suffix": ".bs.js",
  "namespace": true,
  "bs-dependencies": [
    "@rescript/react",
    "bs-fetch",
    "@glennsl/bs-json",
    "@ryyppy/rescript-promise",
    "decco",
    "rescript-webapi"
  ],
  "ppx-flags": ["decco/ppx"],
  "warnings": {
    "error": "+101"
  },
  "reason": {
    "react-jsx": 3
  },
  "js-post-build": {
    "cmd": "yarn post-build"
  }
}
